{
  "name": "rosspens",
  "version": "0.0.1",
  "engines": {
    "node": "10.9.0",
    "npm": "5.7.1'"
  },
  "description": "Rosspens.com: Your source for reliable, affordable vintage pens.",
  "main": "app.js",
  "dependencies": {
    "aws-sdk": "^2.296.0",
    "body-parser": "^1.18.2",
    "connect-mongodb-session": "^2.0.3",
    "ejs": "^2.5.7",
    "ejs-lint": "^0.3.0",
    "express": "^4.16.1",
    "express-formidable": "^1.0.0",
    "express-sanitizer": "^1.0.2",
    "express-session": "^1.15.6",
    "formidable": "^1.1.1",
    "method-override": "^2.3.10",
    "mongoose": "^4.12.0",
    "multer": "^1.3.0",
    "nodemon": "^1.18.3",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^4.2.1",
    "passport-mongoose": "0.0.1",
    "redis": "^2.8.0",
    "slugify": "^1.3.1"
  },
  "devDependencies": {
    "autoprefixer": "^9.1.3",
    "node-sass": "^4.9.3",
    "npm-run-all": "^4.1.3",
    "postcss-cli": "^6.0.0"
  },
  "scripts": {
    "preinstall": "npm install pm2 -g",
    "start": "pm2-runtime app.js -i -1",
    "watch:sass": "node-sass sass/main.scss public/assets/css/rosspens.css -w",
    "compile:sass": "node-sass sass/main.scss public/assets/css/rosspens.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' public/assets/css/rosspens.css -o public/assets/css/rosspens.prefix.css",
    "compress:css": "node-sass public/assets/css/rosspens.prefix.css public/assets/css/rosspens.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "author": "Tucker W. McKinney",
  "license": "ISC"
}
